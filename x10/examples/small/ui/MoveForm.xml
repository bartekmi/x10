<MoveForm model="Move" url="/moves" query="GetMove(tenantId: $tenantId">
  <Form>
    <Heading1>=IsNewObject(id) ? "New Move" : "Editing Move"</Heading1>
    <date/>
    <from/>
    <to/>
    <tenant/>

    <RepellingRow>
      <Text>* Required</Text>
      <SubmitButton label="Save">
        <SubmitButton.Action>
          <Action backEndTarget="/moves/create" successUrl="/moves"/>
        </SubmitButton.Action>
      </SubmitButton>
    </RepellingRow>
  </Form>
</MoveForm>
