<Moves model="Move" many="true" url="/moves" query="Moves">
  <VerticalStackPanel>
    <Heading1 text="Moves"/>

    <Table>
      <Table.Columns>
        <date/>
        <from.name label="From"/>
        <to.name label="To"/>
        <tenant.name label="Tenant"/>
        <TableColumn label="Action">
          <Row>
            <Button label="View" style="link" url="='/moves/view/' + id"/>
            <Button label="Edit" style="normal" url="='/moves/edit/' + id"/>
          </Row>
        </TableColumn>
      </Table.Columns>
    </Table>
  </VerticalStackPanel>
</Moves>
