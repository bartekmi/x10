<Moves model="Move" many="true" url="/moves" query="Moves">
  <VerticalStackPanel>
    <Heading1 text="Moves"/>

    <Table>
      <Table.Columns>
        <date/>
        <from.name label="From"/>
        <to.name label="From"/>
        <tenant.name label="Tenant"/>
        <TableColumn label="Action">
          <Button label="Edit" url="='/moves/edit/' + id"/>
        </TableColumn>
      </Table.Columns>
    </Table>
  </VerticalStackPanel>
</Moves>
