    "webpack-command": "^0.5.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "scripts": {
    "react-lib": "rsync -avu ../react_lib/ src/react_lib",
    "react-lib-back": "rsync -avu src/react_lib/ ../react_lib",
    "react-lib-dry-run": "rsync -avu ../react_lib/ src/react_lib",
    "build": "yarn react-lib && yarn webpack --config config/webpack-dev-config.js",
    "start": "yarn react-lib && node config/webpack-dev-server.js",
    "relay-small": "relay-compiler --src ./x10_generated/small --schema ../SmallSample.graphql --customScalars.DateTime=String --extensions js jsx",
    "relay-dps": "relay-compiler --src ./x10_generated/dps --schema ../Dps.graphql --customScalars.DateTime=String --extensions js jsx",
    "relay-cp": "relay-compiler --src ./x10_generated/client_page --schema ../ClientPage.graphql --customScalars.DateTime=String --extensions js jsx",
    "flow": "flow",
    "gen": "pushd ../x10 && dotnet run && popd",
    "clean": "rm -rf build/*",
    "build-prod": "yarn clean; webpack --config config/webpack-prod-config.js",
    "start-prod": "yarn build; serve build"
  }
}
