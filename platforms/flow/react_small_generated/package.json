    "html-webpack-plugin": "^4.5.0",
    "path": "^0.12.7",
    "react-hot-loader": "^4.13.0",
    "relay-compiler": "^10.0.1",
    "relay-config": "^10.0.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.65.0",
